<h1>Students Report</h1>

<table>
  <tr class="list_head">
    <th>SID</th>
    <th>Last Name</th>
    <th>First Names</th>
    <th>Number of Reports Uploaded</th>
    <th>Action</th>
  </tr>

<% @students.each do |s| %>
  <tr>
    <td><%= s["sid"] %></td>
    <td><b><%= s["last_name"] %></b></td>
    <td><%= s["first_name"] %></td>
    <td><%= s["report_num"] %></td>
    <td><%= link_to 'View Reports', :action => 'detail', :student_id => s["id"]  %></td>
  </tr>
<% end %>

</table>